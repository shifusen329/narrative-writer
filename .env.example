# OpenAI API key for GPT-4o and GPT-4o-mini
OPENAI_API_KEY=your_openai_key_here

# Anthropic API key for Claude 3 models
ANTHROPIC_API_KEY=your_anthropic_key_here

# Google API key for Gemini models
GEMINI_API_KEY=your_gemini_key_here

# NovelAI credentials (subscription required)
NOVELAI_USERNAME=your_novelai_username
NOVELAI_PASSWORD=your_novelai_password

# Optional: Debug mode (true/false)
DEBUG=false

# Optional: Default model (overrides config.json)
DEFAULT_MODEL=gpt-4o
